<%= provide(:title, 'All users') %>
<h1>All users</h1>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <% if user.name?%>
      <%= link_to user.name, user %> 
</br>
       ID =<%=user.id %> 
</br>
       email =<%=user.email %> 
     <%= link_to "delete", user, method: :delete, confirm: "You sure?" %>
      <% end %>
    </li>
  <% end %>
</ul>
